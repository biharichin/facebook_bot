[{"question_text": "\ud835\udde4\ud835\udfed: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddfa\ud835\uddf2\ud835\uddee\ud835\uddfb\ud835\uddf6\ud835\uddfb\ud835\uddf4 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddf2\ud835\uddfb\ud835\uddee\ud835\uddf0\ud835\uddf6\ud835\uddfb\ud835\uddf4\"?", "options": ["Very threatening", "Very small", "Very costly"], "correct_answer_text": "Very threatening"}, {"question_text": "\ud835\udde4\ud835\udfee: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\ude00\ud835\ude06\ud835\uddfb\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddf2\ud835\uddfb\ud835\uddee\ud835\uddf0\ud835\uddf6\ud835\uddfb\ud835\uddf4\"?", "options": ["Frightening, intimidating", "Bright, vivid", "Intelligent, clever"], "correct_answer_text": "Frightening, intimidating"}, {"question_text": "\ud835\udde4\ud835\udfef: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddee\ud835\uddfb\ud835\ude01\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddf2\ud835\uddfb\ud835\uddee\ud835\uddf0\ud835\uddf6\ud835\uddfb\ud835\uddf4\"?", "options": ["Comforting, friendly", "Dull, faded", "Awful, terrible"], "correct_answer_text": "Comforting, friendly"}, {"question_text": "\ud835\udde4\ud835\udff0: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddfa\ud835\uddf2\ud835\uddee\ud835\uddfb\ud835\uddf6\ud835\uddfb\ud835\uddf4 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddee\ud835\uddf7\ud835\uddf2\ud835\ude00\ud835\ude01\ud835\uddf6\ud835\uddf0\"?", "options": ["Very pleasant", "Very grand or beautiful", "Very cold (formal)"], "correct_answer_text": "Very grand or beautiful"}, {"question_text": "\ud835\udde4\ud835\udff1: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\ude00\ud835\ude06\ud835\uddfb\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddee\ud835\uddf7\ud835\uddf2\ud835\ude00\ud835\ude01\ud835\uddf6\ud835\uddf0\"?", "options": ["Awful, terrible", "Magnificent, regal", "Famished, ravenous"], "correct_answer_text": "Magnificent, regal"}, {"question_text": "\ud835\udde4\ud835\udff2: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddee\ud835\uddfb\ud835\ude01\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddee\ud835\uddf7\ud835\uddf2\ud835\ude00\ud835\ude01\ud835\uddf6\ud835\uddf0\"?", "options": ["Ordinary, plain", "Cold, upsetting", "Energetic, fast"], "correct_answer_text": "Ordinary, plain"}, {"question_text": "\ud835\udde4\ud835\udff3: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddfa\ud835\uddf2\ud835\uddee\ud835\uddfb\ud835\uddf6\ud835\uddfb\ud835\uddf4 \ud835\uddfc\ud835\uddf3 \"\ud835\udde7\ud835\uddff\ud835\uddf2\ud835\uddfb\ud835\uddf1\ud835\uddf6\ud835\uddfb\ud835\uddf4 (\ud835\uddd4\ud835\uddf1\ud835\uddf7)\"?", "options": ["A person who affects others\u2019 buying or opinions", "Stored or managed online instead of locally", "Currently popular or widely discussed"], "correct_answer_text": "Currently popular or widely discussed"}, {"question_text": "\ud835\udde4\ud835\udff4: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\ude00\ud835\ude06\ud835\uddfb\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde7\ud835\uddff\ud835\uddf2\ud835\uddfb\ud835\uddf1\ud835\uddf6\ud835\uddfb\ud835\uddf4 (\ud835\uddd4\ud835\uddf1\ud835\uddf7)\"?", "options": ["Fashionable, viral", "Soaked, saturated", "Grief-stricken, heartbroken"], "correct_answer_text": "Fashionable, viral"}, {"question_text": "\ud835\udde4\ud835\udff5: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddee\ud835\uddfb\ud835\ude01\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde7\ud835\uddff\ud835\uddf2\ud835\uddfb\ud835\uddf1\ud835\uddf6\ud835\uddfb\ud835\uddf4 (\ud835\uddd4\ud835\uddf1\ud835\uddf7)\"?", "options": ["Modern, recent", "Outdated, forgotten", "Fix, perfection"], "correct_answer_text": "Outdated, forgotten"}]