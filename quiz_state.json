[{"question_text": "\ud835\udde4\ud835\udfed: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddfa\ud835\uddf2\ud835\uddee\ud835\uddfb\ud835\uddf6\ud835\uddfb\ud835\uddf4 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddf2\ud835\uddfb\ud835\uddee\ud835\uddf0\ud835\uddf6\ud835\uddfb\ud835\uddf4\"?", "options": ["Very threatening", "Spreading rapidly online", "Very hot"], "correct_answer_text": "Very threatening"}, {"question_text": "\ud835\udde4\ud835\udfee: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\ude00\ud835\ude06\ud835\uddfb\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddf2\ud835\uddfb\ud835\uddee\ud835\uddf0\ud835\uddf6\ud835\uddfb\ud835\uddf4\"?", "options": ["Online, web-based", "Lethargic, lazy", "Frightening, intimidating"], "correct_answer_text": "Frightening, intimidating"}, {"question_text": "\ud835\udde4\ud835\udfef: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddee\ud835\uddfb\ud835\ude01\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddf2\ud835\uddfb\ud835\uddee\ud835\uddf0\ud835\uddf6\ud835\uddfb\ud835\uddf4\"?", "options": ["Ugly, plain", "Comforting, friendly", "Unnoticed, obscure"], "correct_answer_text": "Comforting, friendly"}, {"question_text": "\ud835\udde4\ud835\udff0: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddfa\ud835\uddf2\ud835\uddee\ud835\uddfb\ud835\uddf6\ud835\uddfb\ud835\uddf4 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddee\ud835\uddf7\ud835\uddf2\ud835\ude00\ud835\ude01\ud835\uddf6\ud835\uddf0\"?", "options": ["Very shiny", "Spreading rapidly online", "Very grand or beautiful"], "correct_answer_text": "Very grand or beautiful"}, {"question_text": "\ud835\udde4\ud835\udff1: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\ude00\ud835\ude06\ud835\uddfb\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddee\ud835\uddf7\ud835\uddf2\ud835\ude00\ud835\ude01\ud835\uddf6\ud835\uddf0\"?", "options": ["Rash, thoughtless", "Magnificent, regal", "Soaked, saturated"], "correct_answer_text": "Magnificent, regal"}, {"question_text": "\ud835\udde4\ud835\udff2: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddee\ud835\uddfb\ud835\ude01\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde0\ud835\uddee\ud835\uddf7\ud835\uddf2\ud835\ude00\ud835\ude01\ud835\uddf6\ud835\uddf0\"?", "options": ["Ordinary, plain", "Ugly, plain", "Stupid, dull"], "correct_answer_text": "Ordinary, plain"}, {"question_text": "\ud835\udde4\ud835\udff3: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddfa\ud835\uddf2\ud835\uddee\ud835\uddfb\ud835\uddf6\ud835\uddfb\ud835\uddf4 \ud835\uddfc\ud835\uddf3 \"\ud835\udde7\ud835\uddff\ud835\uddf2\ud835\uddfb\ud835\uddf1\ud835\uddf6\ud835\uddfb\ud835\uddf4 (\ud835\uddd4\ud835\uddf1\ud835\uddf7)\"?", "options": ["Very pale", "Very cruel or violent", "Currently popular or widely discussed"], "correct_answer_text": "Currently popular or widely discussed"}, {"question_text": "\ud835\udde4\ud835\udff4: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\ude00\ud835\ude06\ud835\uddfb\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde7\ud835\uddff\ud835\uddf2\ud835\uddfb\ud835\uddf1\ud835\uddf6\ud835\uddfb\ud835\uddf4 (\ud835\uddd4\ud835\uddf1\ud835\uddf7)\"?", "options": ["Starving, famished", "Quick, swift", "Fashionable, viral"], "correct_answer_text": "Fashionable, viral"}, {"question_text": "\ud835\udde4\ud835\udff5: \ud835\uddea\ud835\uddf5\ud835\uddee\ud835\ude01 \ud835\uddf6\ud835\ude00/\ud835\uddee\ud835\uddff\ud835\uddf2 \ud835\ude01\ud835\uddf5\ud835\uddf2 \ud835\uddee\ud835\uddfb\ud835\ude01\ud835\uddfc\ud835\uddfb\ud835\ude06\ud835\uddfa\ud835\ude00 \ud835\uddfc\ud835\uddf3 \"\ud835\udde7\ud835\uddff\ud835\uddf2\ud835\uddfb\ud835\uddf1\ud835\uddf6\ud835\uddfb\ud835\uddf4 (\ud835\uddd4\ud835\uddf1\ud835\uddf7)\"?", "options": ["Corporation, enterprise", "Outdated, forgotten", "Spacious, roomy"], "correct_answer_text": "Outdated, forgotten"}]